<div class="m-3">
  <%= simple_form_for([@lair, @booking]) do |f| %>
    <%= f.input :start_date, as: :string, class: "datepicker", input_html: { data: { controller: "datepicker" } } %>
    <%= f.input :end_date, as: :string, class: "datepicker", input_html: { data: { controller: "datepicker" } } %>
    <%= f.button :submit, class: 'btn btn-primary' %>
  <% end %>
</div>
